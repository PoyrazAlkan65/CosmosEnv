<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>User List</title>
</head>
<body>
    <h1>User List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Email</th>
                <th>UserName</th>
                <th>PhoneNo</th>
                <th>IsActive</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr>
                <td>{{Email}}</td>
                <td>{{UserName}}</td>
                <td>{{PhoneNo}}</td>
                <td>{{IsActive}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</body>
</html>
